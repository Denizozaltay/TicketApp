generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ticket {
  id         String   @id @default(cuid())
  username   String
  title      String
  content    String
  createdAt  DateTime @default(now())
  isArchived Boolean  @default(false)
}
